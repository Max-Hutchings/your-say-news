            </column>
            <column name="universitySubjectEnum" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="propertyOwner" type="BOOLEAN">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
